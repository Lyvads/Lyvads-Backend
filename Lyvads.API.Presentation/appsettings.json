{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Cors": "Debug"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "DB_CONNECTION_STRING"
  },
  "JWT": {
    "Key": "JWT_KEY",
    "Issuer": "JWT_ISSUER",
    "Audience": "JWT_AUDIENCE"
  },
  "Stripe": {
    "PubKey": "STRIPE_PUBKEY",
    "SecretKey": "STRIPE_SECRETKEY"
  },
  "Paystack": {
    "PaystackSK": "PAYSTACK_SK"
  },
  "CloudinarySettings": {
    "CloudName": "CLOUDINARY_CLOUD_NAME",
    "ApiKey": "CLOUDINARY_API_KEY",
    "ApiSecret": "CLOUDINARY_API_SECRET"
  },
  "EmailSettings": {
    "SenderEmail": "EMAIL_SENDER",
    "Port": "EMAIL_PORT",
    "Host": "EMAIL_HOST",
    "AppPassword": "EMAIL_APP_PASSWORD"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/development-.txt",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {Username} {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Lyvads_Client_URL": "http://example.com"
}
